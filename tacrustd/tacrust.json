    {
      "name": "group-superuser",
      "acl": "testacl_permit_all_deny_none",
      "member": ["group-user"],
      "service": [
        {
          "name": "exec",
          "args": [
            "priv-lvl=15"
          ]
        }
      ],
      "cmds": [
        {
          "name": "show",
          "list": [
            "permit \"run\"",
            "permit version",
            "deny"
          ]
        }
      ]
    },
    {
      "name": "group-user",
      "acl": "testacl_permit_all_deny_none",
      "service": [
        {
          "name": "exec",
          "args": [
            "priv-lvl=15"
          ]
        },
        {
          "name": "ppp",
          "args": [
            "F5-LTM-User-Info-1=admin"
          ]
        },
        {
          "name": "raccess",
          "args": [
            "groupname=admin"
          ]
        },
        {
          "name": "junos-exec",
          "args": [
            "allow-commands=\"^.*\"",
            "allow-configuration=\"^.*\""
          ]
        },
        {
          "name": "fortigate",
          "args": [
            "memberof=FGT_admin",
            "admin_prof=super_admin",
            "extra_arg=redundant_value"
          ]
        }
      ],
      "cmds": [
        {
          "name": "show",
          "list": [
            "permit \"clock\"",
            "deny"
          ]
        },
        {
          "name": "dir",
          "list": [
            "permit \"/home.*\"",
            "deny"
          ]
        },
        {
          "name": "file",
          "list": [
            "permit ls.*",
            "permit \"cat.*\"",
            "deny"
          ]
        }
      ]
    },
